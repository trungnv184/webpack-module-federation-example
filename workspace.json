    "shell": {
      "root": "apps/shell",
      "sourceRoot": "apps/shell/src",
      "projectType": "application",
      "targets": {
        "start-all": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "ws --port 4000 --directory dist/apps/shell"
              },
              {
                "command": "ws --port 4001 --directory dist/apps/dashboard"
              },
              {
                "command": "ws --port 4003 --directory dist/apps/login"
              },
              {
                "command": "ws --port 4002 --directory dist/apps/todo"
              },
              {
                "command": "ws --port 4004 --directory dist/apps/about"
              },
              {
                "command": "nx serve static"
              }
            ],
            "parallel": true
          }
        },
        "build-all": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx build about"
              },
              {
                "command": "nx build dashboard"
              },
              {
                "command": "nx build login"
              },
              {
                "command": "nx build shell"
              },
              {
                "command": "nx build todo"
