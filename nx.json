{
  "npmScope": "mfe-example",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "package",
          "prepare"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "@mfes/about": {
      "implicitDependencies": []
    },
    "@mfes/dashboard": {
      "implicitDependencies": []
    },
    "@mfes/login": {
      "implicitDependencies": []
    },
    "@mfes/shell": {
      "implicitDependencies": []
    },
    "@mfes/static": {
      "implicitDependencies": []
    },
    "@mfes/todo": {
      "implicitDependencies": []
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "libsDir": "packages",
    "appsDir": "packages"
  }
}